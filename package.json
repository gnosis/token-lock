{
  "name": "token-lock",
  "version": "1.0.0",
  "description": "Lock ERC-20 tokens for a pre-defined amount of time",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "hardhat compile",
    "test": "hardhat test",
    "test:watch": "mocha -w",
    "deploy": "hardhat initialDeploy --network",
    "verify": "hardhat verifyEtherscan --network",
    "upgrade": "hardhat upgrade --network",
    "coverage": "hardhat coverage",
    "lint": "yarn lint:sol && yarn lint:ts",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:ts": "eslint --max-warnings 0 .",
    "fmt:sol": "prettier 'contracts/**/*.sol' -w",
    "fmt:ts": "prettier '(src|test)/**/*.ts' -w",
    "prepack": "yarn build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git"
  },
  "author": "jan-felix.schwarz@gnosis.io",
  "license": "LGPL-3.0+",
  "devDependencies": {
    "@gnosis.pm/zodiac": "1.0.3",
    "@nomiclabs/hardhat-ethers": "2.0.3",
    "@nomiclabs/hardhat-etherscan": "2.1.8",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@openzeppelin/hardhat-upgrades": "1.12.0",
    "@typechain/ethers-v5": "8.0.5",
    "@typechain/hardhat": "^3.1.0",
    "@types/chai": "4.3.0",
    "@types/dotenv": "8.2.0",
    "@types/mocha": "9.0.0",
    "@types/node": "17.0.8",
    "@types/yargs": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "argv": "0.0.2",
    "chai": "4.3.4",
    "debug": "4.3.3",
    "dotenv": "10.0.0",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-no-only-tests": "2.6.0",
    "eslint-plugin-prettier": "4.0.0",
    "ethereum-waffle": "3.4.0",
    "ethers": "5.5.2",
    "hardhat": "2.8.0",
    "hardhat-deploy": "0.9.24",
    "hardhat-gas-reporter": "1.0.6",
    "husky": "7.0.4",
    "prettier": "2.5.1",
    "prettier-plugin-solidity": "1.0.0-beta.19",
    "solc": "0.8.11",
    "solhint": "3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "0.7.17",
    "ts-node": "10.4.0",
    "typechain": "6.1.0",
    "typescript": "4.5.4",
    "yargs": "17.3.1"
  },
  "dependencies": {
    "@gnosis.pm/mock-contract": "4.0.0",
    "@gnosis.pm/safe-contracts": "1.3.0",
    "@openzeppelin/contracts": "4.4.1",
    "@openzeppelin/contracts-upgradeable": "4.4.1"
  },
  "resolutions": {
    "bitcore-lib": "8.25.0"
  }
}
